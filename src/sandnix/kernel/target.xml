<?xml version="1.0" encoding="utf-8"?><target name="kernel" type="build">
    <output name="kernel"/>
    <outdir path="../../../bin/$(ARCH)/sandnix"/>
    <middir path="../../../obj/$(ARCH)/sandnix"/>
    <introduction>
        Sandnix kernel
    </introduction>
    <archs>
        <arch name="x86">
            <PREV></PREV>
            <DEP>gcc</DEP>
            <DEPRULE>$(DEP) -MM -o $@ $&lt;;if [ $$? == 0 ];then cat $@|awk 'NR == 1 {printf(&quot;%s%s\n&quot;,&quot;$(OBJDIR)/$(ARCH)/&quot;,$$0)} NR != 1 {print $$0}' &gt; $@; fi</DEPRULE>
            <CC>gcc</CC>
            <CFLAGS>-Wall -DX86 -Werror-implicit-function-declaration -Wunused-parameter -std=c99 -m32 -nostdlib -c -Werror -mno-sse2 -mno-sse</CFLAGS>
            <CCRULE>$(CC) $(CFLAGS) -o $@ $&lt;</CCRULE>
            <AS>gcc</AS>
            <ASFLAGS>-c -DX86 -D_ASM -Wall -Werror-implicit-function-declaration -Wunused-parameter -std=c99 -m32 -nostdlib -Werror</ASFLAGS>
            <ASRULE>$(AS) $(ASFLAGS) -o $@ $&lt;</ASRULE>
            <LD>ld</LD>
            <LDFLAGS>-m elf_i386 -Ttext 0xc0101000 -e _start --no-omagic -Map $(OUTDIR)/kernel.map</LDFLAGS>
            <LDRULE>$(LD) $(LDFLAGS) -o $@ $^</LDRULE>
            <AFTER>cp $(LINKED) $(OUTDIR)/$(OUTPUT)</AFTER>
        </arch>
        <arch name="arm.samsung_exynos4412.itop_4412">
            <PREV></PREV>
            <DEP>arm-none-eabi-gcc</DEP>
            <DEPRULE>$(DEP) -MM -o $@ $&lt;;if [ $$? == 0 ];then cat $@|awk 'NR == 1 {printf(&quot;%s%s\n&quot;,&quot;$(OBJDIR)/$(ARCH)/&quot;,$$0)} NR != 1 {print $$0}' &gt; $@; fi</DEPRULE>
            <CC>arm-none-eabi-gcc</CC>
            <CFLAGS>-Wall -DARM -DARM_SAMSUNG_EXYNOS4412 -DARM_SAMSUNG_EXYNOS4412_ITOP_4412 -Werror-implicit-function-declaration -Wunused-parameter -std=c99 -nostdlib -c -Werror -march=armv7-a</CFLAGS>
            <CCRULE>$(CC) $(CFLAGS) -o $@ $&lt;</CCRULE>
            <AS>arm-none-eabi-gcc</AS>
            <ASFLAGS>-c -DARM -DARM_SAMSUNG_EXYNOS4412 -DARM_SAMSUNG_EXYNOS4412_ITOP_4412 -D_ASM -Wall -Werror-implicit-function-declaration -Wunused-parameter -std=c99 -nostdlib -Werror</ASFLAGS>
            <ASRULE>$(AS) $(ASFLAGS) -o $@ $&lt;</ASRULE>
            <LD>arm-none-eabi-ldc</LD>
            <LDFLAGS>-Ttext 0xc0100000 -e _start --no-omagic</LDFLAGS>
            <LDRULE>$(LD) $(LDFLAGS) -o $@ $^</LDRULE>
            <AFTER>cp $(LINKED) $(OUTDIR)/$(OUTPUT)</AFTER>
        </arch>
    </archs>
    <dependencies>
    </dependencies>
    <sub-targets>
    </sub-targets>
    <options>
        <option name="kernel hacking." type="menu">
            <option enable="true" name="Build with symbols." target="CFLAGS|ASFLAGS" type="checkbox" value="-g"/>
        </option>
        <option name="BootLoader." type="menu">
            <option name="Bootloader" selected="0" target="CFLAGS|ASFLAGS" type="list">
            <item name="multiboot2" value="-DBOOTLOADER=MULTIBOOT2"/>
            <item name="uboot" value="-DBOOTLOADER=UBOOT"/>
            </option>
        </option>
    </options>
</target>